<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FunctionBilder.Dekstop.FunctionWindow"
        Title="Function"
        WindowState="Normal"
        WindowStartupLocation="CenterScreen"
        Icon="/icons/functionIcon.png">
  <ScrollViewer IsVisible="True">
  <DockPanel Background="#D46AD0">   
      <Canvas x:Name="BigFunctionCanvas" PropertyChanged="Canvas_SizeChanged"
              PointerPressed="MousePress"
              PointerReleased="MouseUnpress">
      </Canvas>    
  </DockPanel>
  </ScrollViewer>
</Window>
